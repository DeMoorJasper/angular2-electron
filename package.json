{
  "name": "app",
  "productName": "App",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist coverage out && npm cache clean",
    "clean-generated": "rimraf dist doc coverage out",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "npm run package-e2e && protractor",
    "e2e-live": "npm run package-e2e && protractor --elementExplorer",
    "package-e2e": "node scripts/packager.js --platform=current",
    "pretest": "npm run lint",
    "test": "standard && karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run build && npm run test && npm run e2e",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "rimraf dist && webpack --progress --profile --bail && electron .",
    "start-watch": "rimraf dist && webpack --progress --profile --bail --watch",
    "postinstall": "npm run webdriver-update && typings install"
  },
  "dependencies": {
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.3.2",
    "@angular/core": "^4.3.2",
    "@angular/http": "^4.3.2",
    "@angular/platform-browser": "^4.3.2",
    "@angular/platform-browser-dynamic": "^4.3.2",
    "@angular/router": "^4.3.2",
    "core-js": "^2.4.1",
    "debug": "^2.2.0",
    "electron-connect": "^0.6.1",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "standard": "^10.0.2",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "codelyzer": "3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^4.1.1",
    "json-loader": "^0.5.3",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-electron-launcher": "^0.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "null-loader": "0.1.1",
    "on-build-webpack": "^0.1.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^2.0.6",
    "protractor": "^5.1.2",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.0",
    "style-loader": "^0.18.2",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^2.3.2",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.8.0",
    "typescript": "^2.0.9",
    "typings": "^2.1.1",
    "url-loader": "^0.5.6",
    "webpack": "^3.4.1"
  },
  "standard": {
    "globals": [
      "jasmine"
    ],
    "ignore": [
      "src/*"
    ]
  }
}
